  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn start client\"",
    "client": "cd client && yarn start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn build",
    "heroku-postbuild": "yarn build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "antd": "^3.12.3",
    "async": "^2.6.1",
    "bootstrap": "^4.2.1",
    "elemental": "^0.6.1",
    "enzyme": "^3.8.0",
    "express": "^4.16.3",
    "heroku-cli": "^7.0.9",
    "if-env": "^1.0.4",
    "mongodb": "^3.1.12",
    "reactstrap": "^7.1.0"
  }
}
