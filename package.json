{
  "name": "TalkNerdytoMe",
  "version": "1.0.0",
  "description": "Project Three",
  "main": "index.js",
  "repository": "git@github.com:LooLoo5/TalkNerdytoMe.git",
  "author": "Emily and Jason",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"yarn start client\"",
    "client": "cd client && yarn start",
    "install": "cd client && yarn install",
    "build": "cd client && yarn build",
    "heroku-postbuild": "yarn build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "express": "^4.16.3",
    "heroku-cli": "^7.0.9",
    "if-env": "^1.0.4"
  }
}
